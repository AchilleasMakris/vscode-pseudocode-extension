{
  "name": "greekpseudocode",
  "displayName": "greekpseudocode",
  "publisher": "GreekPseudoCode",
  "repository": {
    "type": "git",
    "url": "https://github.com/AchilleasMakris/vscode-pseudocode-extension"
  },
  "icon": "icon/Pseudoglwssa.jpg",
  "keywords": [
    "theme",
    "Formatters",
    "dark theme",
    "pseudocode",
    "pseudoglossa",
    "pseudwglwssa",
    "eap",
    "pseudo"
  ],
  "description": "Highlight the Pseudocode in Greek",
  "version": "1.7.6",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Formatters"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pseudocode",
        "aliases": [
          "Pseudocode",
          "pseudocode"
        ],
        "extensions": [
          ".eap"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pseudocode",
        "scopeName": "source.greek-pseudocode",
        "path": "./syntaxes/pseudocode.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Greek Pseudocode Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/GreekPseudocodeTheme-color-theme.json"
      }
    ],
    "snippets": [
      {
        "language": "pseudocode",
        "path": "./snippets.json"
      }
    ],
    "commands": [
      {
        "command": "greekpseudocode.runEap",
        "title": "Run EAP Program",
        "icon": "icon/play-dark.svg"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "greekpseudocode.runEap",
          "when": "resourceExtname == .eap",
          "group": "navigation"
        }
      ],
      "statusBar/primary": [
        {
          "command": "greekpseudocode.runEap",
          "when": "resourceExtname == .eap"
        }
      ]
    }
  },
  "main": "./extension.js"
}
